@model IEnumerable<UsuariosAleatorios.Models.usuario>

@{
    ViewBag.Title = "UsuarioView";
}

<h2>Lista de usuarios</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomeCompleto)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeCompleto)
            </td>

            <td>
                @Html.ActionLink("Detalhes", "Details", new { uuid = item.login.uuid } )
            </td>
        </tr>
    }

</table>
